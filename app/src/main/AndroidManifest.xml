<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.icollege">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main.SeeDownloadActivity"></activity>
        <activity android:name=".Main.TimeTableActivity" />
        <activity android:name=".Start.UserDetails.NewTeacherActivity" />
        <activity android:name=".Start.UserDetails.NewStudentActivity" />
        <activity android:name=".Main.TeacherMainActivity" />
        <activity android:name=".Main.StudentMainActivity" />
        <activity android:name=".Start.LoginActivity" />
        <activity android:name=".Start.SecurityCheckActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>